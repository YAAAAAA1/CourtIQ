{
  "expo": {
    "name": "HoopMaster",
    "slug": "hoopmaster",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "hoopmaster",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "platforms": ["ios", "android", "web"],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/your-project-id"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.hoopmaster-ai-rlnjsqh",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan your meals for nutrition tracking.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to let you select meal photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.rork.hoopmaster_ai_rlnjsqh",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-notifications"
    ],
    "extra": {
      "router": {
        "origin": "https://rork.com/"
      },
      "eas": {
        "projectId": "b8968fd4-d7b2-444c-99da-0492aacc1683"
      }
    }
  }
}
