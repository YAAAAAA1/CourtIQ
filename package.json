{
  "name": "expo-app",
  "main": "./App.js",
  "babel": {
    "extends": "./babel.config.js"
  },
  "version": "1.0.0",
  "scripts": {
    "start": "WATCHMAN_DISABLED=true expo start",
    "start:web": "WATCHMAN_DISABLED=true expo start --web",
    "start:dev": "WATCHMAN_DISABLED=true expo start",
    "start:clean": "WATCHMAN_DISABLED=true expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean": "rm -rf node_modules/.cache && rm -rf .expo && rm -rf dist",
    "reset": "npm run clean && rm -rf node_modules && npm install",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "build:web": "expo export:web",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@hono/trpc-server": "^0.4.0",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/datetimepicker": "7.7.0",
    "@react-native-community/slider": "4.4.2",
    "@react-native-picker/picker": "2.6.1",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.11.0",
    "@supabase/supabase-js": "^2.50.5",
    "@tanstack/react-query": "^5.83.0",
    "@trpc/client": "^11.4.3",
    "@trpc/react-query": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "axios": "^1.10.0",
    "expo": "~50.0.0",
    "expo-blur": "~12.9.0",
    "expo-camera": "~14.1.0",
    "expo-constants": "~15.4.0",
    "expo-dev-client": "~3.3.0",
    "expo-file-system": "~16.0.0",
    "expo-font": "~11.10.0",
    "expo-haptics": "~12.8.0",
    "expo-image": "~1.10.0",
    "expo-image-picker": "~14.7.0",
    "expo-linear-gradient": "~12.7.0",
    "expo-linking": "~6.2.0",
    "expo-location": "~16.5.0",
    "expo-notifications": "~0.27.0",
    "expo-router": "~3.4.10",
    "expo-secure-store": "~12.8.0",
    "expo-splash-screen": "~0.26.0",
    "expo-status-bar": "~1.11.0",
    "expo-symbols": "~0.2.0",
    "expo-system-ui": "~2.9.0",
    "hono": "^4.8.4",
    "lucide-react-native": "^0.475.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.6",
    "superjson": "^2.2.2",
    "zod": "^4.0.5",
    "zustand": "^5.0.2"
