    "cssnano": "^7.1.0",
    "expo-doctor": "^1.13.5",
    "metro": "~0.80.4",
    "metro-config": "~0.80.4",
    "metro-resolver": "~0.80.4",
    "nativewind": "^4.1.23",
    "postcss": "^8.5.6",
    "postcss-nesting": "^13.0.2",
    "react-native-svg-transformer": "^1.5.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.0.0"
