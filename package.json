{
  "name": "expo-app",
  "main": "App.jsx",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "ulimit -n 65536 && WATCHMAN_DISABLED=true npx expo start --clear",
    "start:web": "WATCHMAN_DISABLED=true npx expo start --web",
    "start:dev": "ulimit -n 65536 && WATCHMAN_DISABLED=true npx expo start --clear",
    "start:clean": "ulimit -n 65536 && WATCHMAN_DISABLED=true npx expo start --clear",
    "start:legacy": "WATCHMAN_DISABLED=true expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean": "rm -rf node_modules/.cache && rm -rf .expo && rm -rf dist",
    "reset": "npm run clean && rm -rf node_modules && npm install",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "build:web": "expo export:web",
    "prebuild": "npm run clean",
    "postinstall": "node patch-native-animated.js"
  },
