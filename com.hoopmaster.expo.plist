<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.hoopmaster.expo</string>
    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>-c</string>
        <string>ulimit -n 65536 && cd /Users/jonahdovdavany/Downloads/TheOfficialHoopMaster && node start-server.js</string>
    </array>
    <key>RunAtLoad</key>
    <false/>
    <key>KeepAlive</key>
